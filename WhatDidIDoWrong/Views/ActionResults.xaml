<UserControl x:Class="WhatDidIDoWrong.Views.ActionResults"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WhatDidIDoWrong.Views"
             mc:Ignorable="d" 
             Background="BurlyWood"
             DataContext="{Binding Main, Source={StaticResource Locator}}"
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Margin="6" Height="Auto">
        <TextBox Name="RawClaims" Height="Auto" VerticalAlignment="Top" 
                 Text="{Binding JsonResponse}"></TextBox>
    </Grid>
</UserControl>
